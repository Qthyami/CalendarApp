<Project Sdk="Microsoft.NET.Sdk.BlazorWebAssembly">

  <PropertyGroup>
    <TargetFramework>net10.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <OverrideHtmlAssetPlaceholders>true</OverrideHtmlAssetPlaceholders>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" Version="10.0.2" />
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.DevServer" Version="10.0.2" PrivateAssets="all" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="wwwroot\sample-data\" />
  </ItemGroup>

  
  <ItemGroup>
    <None Update="Pages\Calendar.razor.css">
      <DependentUpon>Calendar.razor</DependentUpon>
    </None>
    <None Update="Pages\DropdownButton.razor.css">
      <DependentUpon>DropdownButton.razor</DependentUpon>
    </None>
    <None Update="Pages\Cells.razor.css">
      <DependentUpon>Cells.razor</DependentUpon>
    </None>
    <None Update="Pages\DayCell.razor.css">
      <DependentUpon>DayCell.razor</DependentUpon>
    </None>
  </ItemGroup>

</Project>
