<calendar class="calendar-container calendar-popup">
    <CalendarHeader @bind-SelectedMonthStartDate="SelectedMonthStartDate" />
@* <MonthNavigation /> *@
<Cells />
</calendar>
@code {
    [Parameter] public DateTime Date { get; set; }
    [Parameter] public EventCallback<DateTime> DateChanged { get; set; }
    private DateTime SelectedMonthStartDate { get; set; }
    protected override void OnInitialized()
    {
        SelectedMonthStartDate = new DateTime(Date.Year, Date.Month, 1);
    }

}
